{
  "name": "IhatePhish - Do not fall for phishing attacks",
  "version": "1.0",
  "description": "Reminds user to look at url before entering credentials",
  "browser_action": {
    "default-icon" : {
      "16" : "images/no-fish16.png",
      "32" : "images/no-fish32.png"
    },
    "default-title" : "IhatePhish - Do not fall for phishing attacks"

  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["main.css"]
    }
  ],
  "icons": {
    "16" : "images/no-fish16.png",
    "32" : "images/no-fish32.png",
    "128" : "images/no-fish128.png"
  },
  "web_accessible_resources":["images/*.png","videos/*.mp4"],
  "manifest_version": 2
}
